{
    "version": 2,
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/favicon.ico",
            "dest": "/src/public/favicon.ico"
        },
        {
            "src": "/favicon-16x16.png",
            "dest": "/src/public/favicon-16x16.png"
        },
        {
            "src": "/favicon-32x32.png",
            "dest": "/src/public/favicon-32x32.png"
        },
        {
            "src": "/apple-touch-icon.png",
            "dest": "/src/public/apple-touch-icon.png"
        },
        {
            "src": "/site.webmanifest",
            "dest": "/src/public/site.webmanifest"
        },
        {
            "src": "/(.*)",
            "dest": "app.js"
        }
    ]
}